<apex:page standardController="Candidate_Poojan__c">
  <apex:form >
    <apex:pageBlock title="Candidate_Poojan Edit" mode="edit" >
       <apex:pageBlockButtons > 
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!edit}" value="Edit"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="Information" columns="1">
        <apex:inputField value="{!Candidate_Poojan__c.Salutation__c }"/>
        <apex:inputField value="{!Candidate_Poojan__c.First_Name__c }"/>
        <apex:inputField value="{!Candidate_Poojan__c.Last_Name__c }"/>
        <!--<apex:inputField value="{!Candidate_Poojan__c.Full_Name__c}"/>-->
        <apex:inputField value="{!Candidate_Poojan__c.DOB__c}"/>
        <!--<apex:inputField value="{!Candidate_Poojan__c.Age__c}"/>-->
        <apex:inputField value="{!Candidate_Poojan__c.Email__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.Country__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.State__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.Application_Date__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.Status__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.Expected_Salary__c}"/>
        <apex:inputField value="{!Candidate_Poojan__c.Job__c}"/>
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>